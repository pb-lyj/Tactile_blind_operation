{
  "data": {
    "data_root": "datasets/data25.7_aligned",
    "categories": [
      "cir_lar"
    ],
    "train_split": 0.8,
    "batch_size": 4,
    "num_workers": 1,
    "start_frame": 0,
    "normalize_config": {
      "forces": "zscore",
      "actions": "minmax",
      "end_states": null,
      "resultants": null
    }
  },
  "model": {
    "feature_dim": 128,
    "horizon": 3,
    "action_dim": 3,
    "hidden_dims": [
      256,
      128
    ],
    "dropout_rate": 0.25,
    "pretrained_encoder_path": "tactile_representation/prototype_library/cnnae_crt_128.pt"
  },
  "loss": {
    "loss_type": "mse",
    "step_weights": null
  },
  "training": {
    "epochs": 3,
    "lr": 0.0001,
    "weight_decay": 0.0001,
    "eval_every": 1,
    "save_every": 2,
    "patience": 10
  },
  "output": {
    "output_dir": "policy_learn/test_checkpoints"
  },
  "wandb": {
    "enabled": true,
    "mode": "offline",
    "project": "tactile-multistep-test",
    "tags": [
      "test",
      "simplified",
      "action-fix"
    ],
    "notes": "Testing simplified multi-step training with action processing fix"
  }
}